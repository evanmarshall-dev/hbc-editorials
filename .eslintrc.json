{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "func-names": ["warn", "as-needed"],
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/no-cycle": "off"
  }
}
